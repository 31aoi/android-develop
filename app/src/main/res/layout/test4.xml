<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/line1"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent"
        >
    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_weight="1"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/myImageView1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:src="@drawable/a"
            app:layout_constraintTop_toTopOf="@id/line1" />

        <TextView
            android:id="@+id/myTextView1"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:text="SpaceStations"
            android:layout_gravity="center"
/>
    </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/myImageView2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:src="@drawable/rocket_icon"
                app:layout_constraintTop_toTopOf="@id/line1" />

            <TextView
                android:id="@+id/myTextView2"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:text="Flighthts"
                android:layout_gravity="center"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/myImageView3"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:src="@drawable/rover_icon"
                app:layout_constraintTop_toTopOf="@id/line1" />

            <TextView
                android:id="@+id/myTextView3"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:text="Rovers"
                android:layout_gravity="center"
                />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/line2"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/line1">
    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:layout_marginRight="140dp"
        android:background="@color/my_green"
        android:gravity="center"
        android:text="DCA"
        app:layout_constraintBottom_toBottomOf="@+id/line2"
        app:layout_constraintEnd_toEndOf="@+id/line2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/line2" />
    <TextView
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:gravity="center"
        android:background="@color/my_green"
        android:text="DCA"
        android:textColor="@android:color/white"
        android:layout_marginRight="140dp"
        app:layout_constraintBottom_toBottomOf="@+id/line2"
        app:layout_constraintEnd_toEndOf="@+id/line2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/line2"
        />
    <TextView
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:gravity="center"
        android:background="@color/my_green"
        android:text="MARS"
        android:layout_marginLeft="140dp"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="@+id/line2"
        app:layout_constraintEnd_toEndOf="@+id/line2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/line2"
        />
    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/double_arrows"
        app:layout_constraintBottom_toBottomOf="@+id/line2"
        app:layout_constraintEnd_toEndOf="@+id/line2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/line2" />

    <TextView
        android:id="@+id/button1"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:textColor="@android:color/white"
        android:background="@color/my_green"
        android:gravity="center"
        android:text="DEPART"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/image4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/galaxy"
        app:layout_constraintBottom_toTopOf="@+id/button1"
        app:layout_constraintTop_toBottomOf="@+id/line2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/rocket_icon"
        app:layout_constraintBottom_toTopOf="@+id/button1"
        app:layout_constraintEnd_toStartOf="@+id/image4"
        android:layout_marginStart="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line2" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="159dp"
        android:layout_height="36dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="220dp"
        android:text="One Way"
        android:background="@color/my_orange"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.029"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037" />

    <TextView
        android:id="@+id/button3"
        android:layout_width="80dp"
        android:layout_height="28dp"
        android:layout_marginTop="18dp"
        android:gravity="center"
        android:textColor="@android:color/white"
        android:text="1 Traveller"
        android:background="@color/my_orange"
        app:layout_constraintStart_toStartOf="@+id/switch1"
        app:layout_constraintTop_toBottomOf="@+id/switch1" />

</androidx.constraintlayout.widget.ConstraintLayout>